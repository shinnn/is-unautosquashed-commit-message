		"check",
		"if"
	],
	"devDependencies": {
		"@shinnn/eslint-config": "^6.8.9",
		"eslint": "^5.12.1",
		"nyc": "^13.1.0",
		"nyc-config-common": "^1.0.1",
		"rollup": "^1.1.0",
		"rollup-config-module": "^2.0.2",
		"tape": "^4.9.2"
	},
	"eslintConfig": {
		"extends": "@shinnn"
	},
	"nyc": {
		"extends": "nyc-config-common"
	}
}
