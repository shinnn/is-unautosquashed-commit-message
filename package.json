    "check",
    "if"
  ],
  "devDependencies": {
    "@shinnn/eslint-config": "^3.3.6",
    "eslint": "^3.13.1",
    "istanbul": "^0.4.5",
    "rollup": "^0.41.1",
    "rollup-config-module": "^1.0.0",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@shinnn"
  }
}
